{
  "manifest_version": 3,
  "name": "Comment Anywhere",
  "description": "Get and post comments for any page.",
  "version": "0.0.1",
  
  "action": {
    "default_popup": "index.html"
  },
  
  "icons": {
      "98": "icons/CA-Logo.png"
  },
  "permissions": [
    "activeTab",
    "scripting",
    "storage",
    "tabs"
]
}
